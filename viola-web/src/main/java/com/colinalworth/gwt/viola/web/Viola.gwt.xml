<module rename-to="viola">
	<inherits name="com.google.gwt.user.User" />
	<inherits name="com.sencha.gxt.ui.GXT" />

	<inherits name="com.sencha.gxt.theme.neptune.Theme" />


	<inherits name="com.google.gwt.inject.Inject"/>
	<inherits name="com.colinalworth.rpq.RPQ"/>
	<inherits name="de.barop.gwt.PushState" />
	<set-property name="gwt.logging.popupHandler" value="DISABLED" />

	<generate-with class="com.colinalworth.gwt.viola.web.rebind.PlacesGenerator">
		<when-type-assignable class="com.colinalworth.gwt.viola.web.shared.mvp.PlaceManager.PlaceFactory" />
	</generate-with>
	<super-source path="super" />


	<source path="client" />
	<source path="shared" />


	<entry-point class="com.colinalworth.gwt.viola.web.client.ViolaApp" />
	<collapse-all-properties />
	<add-linker name="xsiframe"/>

</module>